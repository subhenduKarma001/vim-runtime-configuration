#cp - template-1 
snippet cp "cp template-1"
#include<bits/stdc++.h>
using namespace std;

/* #define int long long */
#define endl "\n"

void solve() {
	${0}
}

int32_t main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0); cout.tie(0); 

	int tc=1; 
	cin >> tc;
	for(int i=1; i<=tc; i++) {
		/* cout << "Case #" << i << ": "; */
		solve();
	}

	return 0;
}
endsnippet

